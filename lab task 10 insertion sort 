#include <iostream>
using namespace std;

int main()
{
    int n, i, j, key;
    
    cout << "Enter number of elements: ";
    cin >> n;
    int arr[n];
   
    cout << "Enter elements:\n";
    for(i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    // Insertion Sort 
    for(i = 1; i < n; i++)
    {
        key = arr[i];      // store current element
        j = i - 1;

        while(j >= 0 && arr[j] > key)
        {
            arr[j + 1] = arr[j];  // shift element to right
            j--;
        }
        arr[j + 1] = key; 
    }


    cout << "Sorted Array:\n";
    for(i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }

    return 0;
}